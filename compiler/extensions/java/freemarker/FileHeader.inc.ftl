<#macro file_header generatorDescription>
/**
 * Automatically generated by ${generatorDescription}.
 */
</#macro>

<#macro package packageName>
    <#if packageName?? && packageName != "">
package ${packageName};
    </#if>
</#macro>

<#macro default_imports javaMajorVersion>
import javax.annotation.<#if javaMajorVersion?number &gt; 8>processing.</#if>Generated;
</#macro>

<#macro imports importsList>
    <#list importsList as importSpec>
import ${importSpec};
    </#list>
</#macro>

<#macro class_header generatorDescription>
@Generated(
    value = "zserio.tools.ZserioTool",
    comments = "generated by ${generatorDescription}"
)
</#macro>

<#macro standard_header generatorDescription packageName javaMajorVersion importsList>
<@file_header generatorDescription/>

<@package packageName/>

<@default_imports javaMajorVersion/>
<@imports importsList/>
</#macro>
