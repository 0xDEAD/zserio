<project name="parameterized_types_error" basedir="." default="run">
    <testProperties testPath="errors/parameterized_types_error" testName="parameterized_types_error"/>

    <target name="prepare">
        <testPrepare testName="parameterized_types_error"/>
    </target>

    <target name="gen" depends="prepare">
        <testGen testName="parameterized_types_error" zsFile="referenced_non_compound_type_error.zs"
            ignoreErrors="true"/>
        <testGen testName="parameterized_types_error" zsFile="referenced_non_parametrized_type_error.zs"
            ignoreErrors="true"/>
        <testGen testName="parameterized_types_error" zsFile="referenced_parametrized_type_error.zs"
            ignoreErrors="true"/>
        <testGen testName="parameterized_types_error" zsFile="referenced_unknown_parametrized_type_error.zs"
            ignoreErrors="true"/>
        <testGen testName="parameterized_types_error" zsFile="wrong_number_of_arguments_error.zs"
            ignoreErrors="true"/>
        <testGen testName="parameterized_types_error" zsFile="wrong_argument_type_error.zs"
            ignoreErrors="true"/>
    </target>

    <target name="compile" depends="gen">
        <testCompile testName="parameterized_types_error" srcPath="${parameterized_types_error.base_dir}/java"/>
    </target>

    <target name="findbugs" depends="compile" if="findbugs.home_dir">
        <testFindbugs testName="parameterized_types_error"
            srcPath="${parameterized_types_error.base_dir}/java"/>
    </target>

    <target name="run" depends="findbugs">
        <testRun testName="parameterized_types_error"/>
    </target>

    <target name="clean">
        <testClean testName="parameterized_types_error"/>
    </target>
</project>
